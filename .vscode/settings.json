{
    "cmake.sourceDirectory": "/home/ros2/ws/src/modulo_components",
    "clang-format.fallbackStyle": "none",
    "clang-format.language.cpp.style": "file:/home/ros2/.clang-format",
    "C_Cpp.intelliSenseEngine": "disabled",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format",
        "editor.inlayHints.enabled": "on"
    },
    "python.analysis.typeCheckingMode": "basic",
    "python.languageServer": "Pylance",
    "autopep8.args": [
        "--max-line-length",
        "120",
        "--experimental"
    ],
    "pylint.args": [
        "--generate-members",
        "--max-line-length",
        "120",
        "-d",
        "C0114",
        "-d",
        "C0115",
        "-d",
        "C0116"
    ],
    "editor.rulers": [
        120
    ],
    "yaml.schemas": {
        "https://raw.githubusercontent.com/aica-technology/api/731960c66a8fb9b29620a9f79baaa935818ab1db/docs/static/schemas/1-0-2/extension.schema.json": [
            "extension_descriptions/*.yaml"
        ]
    }
}